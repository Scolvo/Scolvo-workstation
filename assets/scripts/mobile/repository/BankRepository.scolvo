function getBanks() {
  return select()
    .from("bank")
    .orderBy("name", "asc")
    .execute();
}

function getBankById(bankId) {
  var bankDaos = select()
    .from("bank")
    .where(eq("id", bankId))
    .execute();
  if(bankDaos.isEmpty()) {
    return null;
  }
  return bankDaos.get(0);
}
