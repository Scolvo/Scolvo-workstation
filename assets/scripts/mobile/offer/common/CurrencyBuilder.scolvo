var CURRENCY_EUR="eur";
var CURRENCY_RSD="rsd";

function buildCurrencyOptions(selectedCurrency) {
  return [
    buildSelectOptions(CURRENCY_EUR, DICTIONARY.currency_eur, CURRENCY_EUR == selectedCurrency),
    buildSelectOptions(CURRENCY_RSD, DICTIONARY.currency_rsd, CURRENCY_RSD == selectedCurrency)
  ];
}

function resolveCurrency(currencyValue) {
  return DICTIONARY.get("currency_" + currencyValue);
}
