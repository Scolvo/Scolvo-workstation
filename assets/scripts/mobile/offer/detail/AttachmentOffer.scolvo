import {
  /mobile/repository/AttachmentRepository
}

function onAttachmentOffer(originId) {
  displayAttachments("Offer", getAttachmentByOfferId(selectedOfferDao.id), DICTIONARY.page_OfferDetailAttachment_headerText);
}

function onOfferAddAttachment(originId) {
  insertOfferAttachment($IN.data, selectedOfferDao.id);
}

function onOfferDeleteAttachment(originId) {
  deleteAttachmentById($IN.data.id);
}
