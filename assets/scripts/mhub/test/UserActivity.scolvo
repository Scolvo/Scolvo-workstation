import {
  /mhub/DataChange
}

function sendAppStartUserActivity(originId) {
  sendUserActivity(originId, "APPLICATION_START");
}

function sendUserActivity(originId, activityType) {
  var currentUser = getCurrentUser();
  var userActivityDao = {
    "id": uuid(),
    "userId": currentUser.userId,
    "username": currentUser.preferred_username,
    "activityType": activityType,
    "deviceType": "MHUB", // SVM change to support this
    "timestamp": nowInMillis(),
    "changeType": "INSERT"
  };
  sendDataChangeRq(originId, "userActivity", [userActivityDao]);
}
