function getWorkplacesForMhub() {
  return select()
  .selectAs("id")
  .selectAs("teamId")
  .selectAs("name")
  .from("workplace")
  .execute();
}

function getWorkplacesForMobile() {
  return select()
  .selectAs("id")
  .selectAs("teamId")
  .selectAs("name")
  .selectAs("canBeBooked")
  .from("workplace")
  .execute();
}
