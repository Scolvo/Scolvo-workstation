function getMeetingRoomsForMhub() {
  return select()
  .selectAs("id")
  .selectAs("name")
  .selectAs("capacity")
  .selectAs("location")
  .selectAs("mapPictureName")
  .from("meetingRoom")
  .execute();
}

function getMeetingRoomsForMobile() {
  return select()
  .selectAs("id")
  .selectAs("name")
  .selectAs("capacity")
  .selectAs("location")
  .selectAs("mapPictureName")
  .selectAs("library")
  .from("meetingRoom")
  .execute();
}
