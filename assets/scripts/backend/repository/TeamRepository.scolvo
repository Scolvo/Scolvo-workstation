function getTeamsForMhub() {
  return select()
  .selectAs("id")
  .selectAs("name")
  .selectAs("location")
  .from("team")
  .execute();
}

function getTeamsForMobile() {
  return select()
  .selectAs("id")
  .selectAs("name")
  .selectAs("location")
  .selectAs("mapPicturePath")
  .from("team")
  .execute();
}
