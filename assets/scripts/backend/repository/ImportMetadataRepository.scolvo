function getImportMetadata() {
  return select()
    .selectAs("id")
    .selectAs("importType")
    .selectAs("importDate")
    .from("importMetadata")
    .execute();
}

function getImportMetadataByimportTypeAndAge(importType, days) {
  return select()
    .selectAs("importMetadata.id", "id")
    .from("importMetadata")
    .where(and(eq("importType", importType), custom("importDate < (unix_timestamp(DATE_SUB(curdate(), INTERVAL 30 DAY)) * 1000)")))
    .execute();
}
